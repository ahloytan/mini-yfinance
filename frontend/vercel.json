{
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Content-Security-Policy",
              "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://vercel.live; style-src 'self' 'unsafe-inline'; img-src 'self' data: https://tailwindui.com; font-src 'self'; connect-src 'self' https://mini-yfinance-backend.vercel.app *; frame-src 'self' https://vercel.live/;"
            },
            {
              "key": "Permissions-Policy",
              "value": "autoplay=(), camera=(), fullscreen=(), geolocation=(), microphone=()"
            },
            { "key" : "Access-Control-Allow-Origin", "value" : "*" },
            { "key" : "X-Frame-Options", "value" : "deny" },
            { "key": "Referrer-Policy", "value": "no-referrer" },
            { "key" : "X-Content-Type-Options", "value" : "nosniff" },
            { "key" : "X-XSS-Protection", "value" : "1; mode=block" }
          ]
        }
      ]
}